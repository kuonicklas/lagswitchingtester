            "args": [
                "-g",
                "${fileDirname}/../src/server.cpp",
                "-I", "${fileDirname}/../../Libraries/SDL2-2.30.0/x86_64-w64-mingw32/include",
                "-I", "${fileDirname}/../../Libraries/enet-1.3.18/include",
                "-I", "${fileDirname}/../include",
                "-L", "${fileDirname}/../../Libraries/SDL2-2.30.0/x86_64-w64-mingw32/lib",
                "-L", "${fileDirname}/../../Libraries/enet-1.3.18",
                "-o", "${fileDirname}/../server",
                "-l", "mingw32",
                "-l", "SDL2main",
                "-l", "SDL2",
                "-l", "enet64",
                "-l", "ws2_32",
                "-l", "winmm"
              ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
